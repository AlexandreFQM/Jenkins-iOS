# platform :ios, '9.0'

target 'xpto' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for xpto
pod 'SwiftyJSON', '~> 4.0'
  target 'xptoTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SwiftyJSON', '~> 4.0'
  end

  target 'xptoUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SwiftyJSON', '~> 4.0'
  end

end
